@model List<LemonCat.Models.EF.TAIKHOAN>
@{
    ViewBag.Title = "ShowLikeNews";
    Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
}
    <section class="content contact">
        <div class="body_scroll">
            <div class="block-header">
                <div class="row">
                    <div class="col-lg-7 col-md-6 col-sm-12">
                        <h2>User</h2>
                        <ul class="breadcrumb">
                            <li class="breadcrumb-item"><a href="index.html"><i class="zmdi zmdi-home"></i> Manager</a></li>
                            <li class="breadcrumb-item"><a href="javascript:void(0);">Review</a></li>
                            <li class="breadcrumb-item active">Score Review List</li>
                        </ul>
                        <button class="btn btn-primary btn-icon mobile_menu" type="button"><i class="zmdi zmdi-sort-amount-desc"></i></button>
                    </div>
                    <div class="col-lg-5 col-md-6 col-sm-12">
                        <button class="btn btn-primary btn-icon float-right right_icon_toggle_btn" type="button"><i class="zmdi zmdi-arrow-right"></i></button>
                    </div>
                </div>
            </div>
            <div class="container-fluid">
                <div class="row clearfix">
                    <div class="col-lg-12">
                        <div class="card">
                            <div class="table-responsive">
                                <!--js-basic-example <> js-exportable-->
                                <table id="example" class="table table-striped table table-hover mb-0 c_list c_table js-exportable dataTable">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Name</th>
                                            <th>Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @for (int i = 0; i < Model.Count; i++)
                                        {
                                            <tr id="row_@Model[i].MaTK">
                                                <td>
                                                    <p class="c_name">@Model[i].MaTK</p>
                                                </td>
                                                <td>
                                                    <img src="@(Model[i].AnhDaiDien.ToString())" class="avatar w30" alt="Avata">
                                                    <p class="c_name">@Model[i].LastName</p>
                                                </td>
                                                <td>
                                                    <a href="#" class="zmdi zmdi-hc-fw">@(ViewBag.Mark[i] ? "" : "")</a>
                                                </td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>