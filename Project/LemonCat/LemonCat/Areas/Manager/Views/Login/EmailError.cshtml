
@{
    Layout = null;
}

<!doctype html>
<html class="no-js " lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="description" content="Responsive Bootstrap 4 and web Application ui kit.">

    <title>LemonCat Admin 500</title>
    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="~/Areas/Admin/image/x-icon">
    <!-- Custom Css -->
    <link href="~/Areas/Admin/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Areas/Admin/assets/css/style.min.css" rel="stylesheet">
</head>

<body class="theme-blush">

    <div class="authentication">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-sm-12">
                    <form class="card auth_form">
                        <div class="header">
                            <img class="logo" src="assets/images/logo.svg" alt="">
                            <h5>Error 500</h5>
                            <span>Your Email is not in Our Database :-(</span>
                        </div>
                        <div class="body">
                            <a href="/Manager/Login" class="btn btn-primary btn-block waves-effect waves-light">GO TO LOGIN</a>
                            <div class="signin_with mt-3">
                                <a href="javascript:void(0);" class="link">Need Help?</a>
                            </div>
                        </div>
                    </form>
                    <div class="copyright text-center">
                        &copy;
                        <script>document.write(new Date().getFullYear())</script>,
                        <span><a href="templatespoint.net">Lý Thành Đạt - Lê Minh Ngọc</a></span>
                    </div>
                </div>
                <div class="col-lg-8 col-sm-12">
                    <div class="card">
                        <img src="~/Areas/Admin/assets/images/signin.svg" alt="500" />
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Jquery Core Js -->
    <script src="~/Areas/Admin/assets/bundles/libscripts.bundle.js"></script>
    <script src="~/Areas/Admin/assets/bundles/vendorscripts.bundle.js"></script> <!-- Lib Scripts Plugin Js -->
</body>


</html>